{
  "name": "superstatic",
  "version": "1.0.1",
  "description": "Superstatic: a static file server for fancy apps",
  "main": "lib",
  "scripts": {
    "test": "npm run lint && npm run test-unit",
    "test-unit": "mocha test",
    "lint": "jshint ./lib",
    "outdated": "npm outdated --depth 0",
    "audit": "npm shrinkwrap --dev && nsp audit-shrinkwrap || true && rm npm-shrinkwrap.json"
  },
  "bin": {
    "superstatic": "bin/server",
    "ss": "bin/server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/divshot/superstatic.git"
  },
  "keywords": [
    "static",
    "server",
    "divshot",
    "hosting"
  ],
  "author": "Divshot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/divshot/superstatic/issues"
  },
  "dependencies": {
    "async": "~0.9.0",
    "broker": "1.0.1",
    "cache-control": "0.1.1",
    "caseless": "^0.6.0",
    "chalk": "^0.5.1",
    "clean-urls": "1.0.1",
    "compression": "^1.1.0",
    "connect": "^3.2.0",
    "connect-query": "^0.2.0",
    "fast-url-parser": "^1.0.6-0",
    "feedback": "^0.3.2",
    "file-exists": "^0.1.1",
    "lodash": "^2.4.1",
    "minimist": "^1.1.0",
    "mix-into": "~0.4.0",
    "morgan": "^1.3.0",
    "nash": "^0.5.0",
    "not-found": "1.0.1",
    "pretty-print": "^0.4.1",
    "qs": "^2.2.3",
    "redirects": "^0.2.0",
    "serve-favicon": "~2.1.0",
    "set-headers": "0.1.1",
    "slasher": "^0.1.5",
    "slashify": "^0.1.0",
    "stacked": "^1.1.0",
    "static-router": "1.0.1",
    "update-notifier": "^0.2.1",
    "van": "0.0.4"
  },
  "devDependencies": {
    "args-list": "^0.3.2",
    "chai": "~1.9.1",
    "clone": "0.1.x",
    "connect": "3.x",
    "fs-extra": "^0.10.0",
    "jshint": "2.5.x",
    "knox": "~0.8.6",
    "minimatch": "^1.0.0",
    "mkdirp": "0.5.x",
    "mocha": "^1.21.4",
    "mocksy": "~0.1.4",
    "nsp": "~0.2.6",
    "request": "~2.40.0",
    "rmdir": "1.0.x",
    "serve-static": "^1.3.2",
    "sinon": "~1.10.0",
    "supertest": "^0.13.0"
  },
  "jshintConfig": {
    "expr": true,
    "curly": false,
    "eqeqeq": true,
    "immed": true,
    "latedef": false,
    "newcap": true,
    "noarg": true,
    "sub": true,
    "undef": true,
    "unused": false,
    "boss": true,
    "eqnull": true,
    "node": true,
    "laxbreak": true,
    "globals": {
      "require": true,
      "module": true,
      "exports": true,
      "it": true,
      "describe": true,
      "beforeEach": true,
      "afterEach": true,
      "chai": true,
      "Regex": true
    }
  }
}
